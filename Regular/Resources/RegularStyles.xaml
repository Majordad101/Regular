    <Style x:Key="StandardWindow" TargetType="Window">
        <Setter Property="ResizeMode" Value="NoResize" />
        <Setter Property="Topmost" Value="False" />
        <Setter Property="Background" Value="{StaticResource WindowBackgroundColor}" />
        <Setter Property="Icon" Value="../Resources/RegularIcon32px.png" />
    </Style>

    <Style x:Key="StandardText" TargetType="TextBlock">
        <Setter Property="OverridesDefaultStyle" Value="True" />
        <Setter Property="FontFamily" Value="Segoe UI" />
        <Setter Property="FontWeight" Value="Regular" />
        <Setter Property="FontSize" Value="12px" />
        <Setter Property="Foreground" Value="{StaticResource FontColorDefault}" />
    </Style>

    <Style x:Key="WarningTextBox" TargetType="TextBox">
        <Setter Property="FontFamily" Value="Segoe UI" />
        <Setter Property="FontSize" Value="12px" />
        <Setter Property="Foreground" Value="{StaticResource FontColorWarning}" />
    </Style>

    <Style x:Key="StandardLabel" TargetType="{x:Type Label}">
        <Setter Property="FontFamily" Value="Segoe UI" />
        <Setter Property="FontWeight" Value="Regular" />
        <Setter Property="FontSize" Value="12px" />
        <Setter Property="Padding" Value="0px" />
        <Setter Property="Foreground" Value="{StaticResource FontColorDefault}" />
    </Style>

    <Style x:Key="StandardComboBox" TargetType="{x:Type ComboBox}">
        <Setter Property="FontFamily" Value="Segoe UI" />
        <Setter Property="FontWeight" Value="Regular" />
        <Setter Property="FontSize" Value="12px" />
        <Setter Property="Foreground" Value="{StaticResource FontColorDefault}" />
        <Setter Property="Background" Value="White" />
        <Setter Property="Height" Value="22px" />
        <Setter Property="Margin" Value="0px" />
        <Setter Property="Padding" Value="5,0,0,0" />
        <Setter Property="BorderThickness" Value="1px" />
        <Setter Property="VerticalAlignment" Value="Top" />
        <Setter Property="IsReadOnly" Value="True" />
        <Setter Property="IsEditable" Value="True" />
        <Setter Property="VerticalContentAlignment" Value="Center" />
    </Style>

    <Style x:Key="StandardTextBox" TargetType="TextBox">
        <Setter Property="Background" Value="{StaticResource WindowBackgroundColor}" />
        <Setter Property="Foreground" Value="{StaticResource FontColorDefault}" />
        <Setter Property="FontFamily" Value="Segoe UI" />
        <Setter Property="FontWeight" Value="Regular" />
        <Setter Property="FontSize" Value="12px" />
        <Setter Property="Height" Value="22px" />
        <Setter Property="Margin" Value="0px" />
        <Setter Property="Padding" Value="3,0,0,0" />
        <Setter Property="BorderThickness" Value="1px" />
        <Setter Property="VerticalAlignment" Value="Top" />
        <Setter Property="BorderBrush" Value="{StaticResource TextBoxBorderColorDefault}" />
    </Style>

    <Style x:Key="RulePartTextBox" TargetType="TextBox">
        <Setter Property="Background" Value="{StaticResource RulePartBoxColor}" />
        <Setter Property="Foreground" Value="{StaticResource FontColorDefault}" />
        <Setter Property="FontFamily" Value="Segoe UI" />
        <Setter Property="FontWeight" Value="Regular" />
        <Setter Property="FontSize" Value="12px" />
        <Setter Property="Height" Value="22px" />
        <Setter Property="Margin" Value="0px" />
        <Setter Property="Padding" Value="-2,0,0,0" />
        <Setter Property="BorderThickness" Value="0px" />
        <Setter Property="VerticalAlignment" Value="Center" />
        <Setter Property="VerticalContentAlignment" Value="Center" />
        <Setter Property="Width" Value="Auto" />
        <Setter Property="HorizontalAlignment" Value="Stretch" />
        <Setter Property="IsReadOnly" Value="True" />
    </Style>

    <Style x:Key="StandardListBox" TargetType="ListBox">
        <Setter Property="Background" Value="{StaticResource WindowBackgroundColor}" />
        <Setter Property="Foreground" Value="{StaticResource FontColorDefault}" />
        <Setter Property="Margin" Value="0px" />
        <Setter Property="Padding" Value="0px" />
        <Setter Property="BorderThickness" Value="0px" />
        <Setter Property="VerticalAlignment" Value="Stretch" />
    </Style>

    <Style x:Key="FadedText" TargetType="TextBlock">
        <Setter Property="OverridesDefaultStyle" Value="True" />
        <Setter Property="FontFamily" Value="Segoe UI" />
        <Setter Property="FontWeight" Value="Regular" />
        <Setter Property="FontSize" Value="13px" />
        <Setter Property="Foreground" Value="{StaticResource FontColorWatermarkGrey}" />
    </Style>

    <Style x:Key="WarningText" TargetType="TextBlock">
        <Setter Property="OverridesDefaultStyle" Value="True" />
        <Setter Property="FontFamily" Value="Segoe UI" />
        <Setter Property="FontSize" Value="12px" />
        <Setter Property="Foreground" Value="{StaticResource FontColorWarning}" />
    </Style>

    <Style x:Key="StandardButton" TargetType="Button">
        <Setter Property="OverridesDefaultStyle" Value="True" />
        <Setter Property="Template">
            <Setter.Value>
                <ControlTemplate TargetType="{x:Type Button}">
                    <Grid
                        Width="{TemplateBinding Width}"
                        Height="{TemplateBinding Height}"
                        ClipToBounds="True">
                        <Rectangle
                            x:Name="BackgroundRectangle"
                            HorizontalAlignment="Stretch"
                            VerticalAlignment="Stretch"
                            Fill="{StaticResource ButtonBackgroundColorDefault}" />
                        <DockPanel Name="ContentPresenterDockPanel">
                            <ContentPresenter
                                x:Name="ContentPresenter"
                                HorizontalAlignment="Center"
                                VerticalAlignment="Center"
                                Content="{TemplateBinding Content}">
                                <ContentPresenter.Resources>
                                    <Style BasedOn="{StaticResource ResourceKey=StandardText}" TargetType="TextBlock" />
                                </ContentPresenter.Resources>
                            </ContentPresenter>
                        </DockPanel>
                    </Grid>
                    <ControlTemplate.Triggers>
                        <Trigger Property="IsMouseOver" Value="True">
                            <Setter TargetName="BackgroundRectangle" Property="Rectangle.Fill" Value="{StaticResource ButtonBackgroundColorLowlight}" />
                        </Trigger>
                        <Trigger Property="IsEnabled" Value="False">
                            <Setter TargetName="ContentPresenter" Property="TextBlock.Opacity" Value="0.5" />
                        </Trigger>
                    </ControlTemplate.Triggers>
                </ControlTemplate>
            </Setter.Value>
        </Setter>
    </Style>

    <Style x:Key="RulePartButtonControl" TargetType="Button">
        <Setter Property="OverridesDefaultStyle" Value="True" />
        <Setter Property="Template">
            <Setter.Value>
                <ControlTemplate TargetType="{x:Type Button}">
                    <Grid
                        Width="{TemplateBinding Width}"
                        Height="{TemplateBinding Height}"
                        ClipToBounds="True">
                        <Rectangle
                            x:Name="BackgroundRectangle"
                            HorizontalAlignment="Stretch"
                            VerticalAlignment="Stretch"
                            Fill="#D0D0CF"
                            RadiusX="3"
                            RadiusY="3" />
                        <DockPanel Name="ContentPresenterDockPanel">
                            <ContentPresenter
                                x:Name="ContentPresenter"
                                HorizontalAlignment="Center"
                                VerticalAlignment="Center"
                                Content="{TemplateBinding Content}"
                                TextBlock.Foreground="{StaticResource FontColorDefault}">
                                <ContentPresenter.Resources>
                                    <Style BasedOn="{StaticResource ResourceKey=StandardText}" TargetType="TextBlock" />
                                </ContentPresenter.Resources>
                            </ContentPresenter>
                        </DockPanel>
                    </Grid>
                    <ControlTemplate.Triggers>
                        <Trigger Property="IsMouseOver" Value="True">
                            <Setter TargetName="BackgroundRectangle" Property="Rectangle.Fill" Value="{StaticResource ButtonBackgroundColorHighlight}" />
                        </Trigger>
                    </ControlTemplate.Triggers>
                </ControlTemplate>
            </Setter.Value>
        </Setter>
    </Style>

    <Style x:Key="ReorderUpButton" TargetType="Button">
        <Setter Property="OverridesDefaultStyle" Value="True" />
        <Setter Property="Template">
            <Setter.Value>
                <ControlTemplate TargetType="{x:Type Button}">
                    <Canvas
                        Width="{TemplateBinding Width}"
                        Height="{TemplateBinding Height}"
                        HorizontalAlignment="Center"
                        VerticalAlignment="Center"
                        ClipToBounds="True">
                        <Rectangle
                            x:Name="BackgroundRectangle"
                            Width="{TemplateBinding Width}"
                            Height="{TemplateBinding Height}"
                            Margin="0"
                            HorizontalAlignment="Stretch"
                            VerticalAlignment="Stretch"
                            Fill="{StaticResource ButtonBackgroundGray}" />
                        <Line
                            x:Name="Line1"
                            Stroke="{StaticResource ButtonStrokeColorDefault}"
                            StrokeThickness="1.5"
                            X1="8.5"
                            X2="13"
                            Y1="14"
                            Y2="10" />
                        <Line
                            x:Name="Line2"
                            Stroke="{StaticResource ButtonStrokeColorDefault}"
                            StrokeThickness="1.5"
                            X1="12"
                            X2="16.5"
                            Y1="10"
                            Y2="14" />
                        <Ellipse
                            x:Name="ReorderUpButtonEllipse"
                            Canvas.Left="5"
                            Canvas.Top="5"
                            Width="15"
                            Height="15"
                            Stroke="{StaticResource ButtonStrokeColorDefault}"
                            StrokeThickness="1" />
                    </Canvas>
                    <ControlTemplate.Triggers>
                        <Trigger Property="IsEnabled" Value="False">
                            <Setter TargetName="Line1" Property="Line.Stroke" Value="{StaticResource ButtonStrokeColorDisabled}" />
                            <Setter TargetName="Line2" Property="Line.Stroke" Value="{StaticResource ButtonStrokeColorDisabled}" />
                            <Setter TargetName="ReorderUpButtonEllipse" Property="Ellipse.Stroke" Value="{StaticResource ButtonStrokeColorDisabled}" />
                        </Trigger>
                        <Trigger Property="IsMouseOver" Value="True">
                            <Setter TargetName="BackgroundRectangle" Property="Rectangle.Fill" Value="#999999" />
                            <Setter TargetName="ReorderUpButtonEllipse" Property="Ellipse.Stroke" Value="#FFFFFF" />
                            <Setter TargetName="Line1" Property="Line.Stroke" Value="#FFFFFF" />
                            <Setter TargetName="Line2" Property="Line.Stroke" Value="#FFFFFF" />
                        </Trigger>
                    </ControlTemplate.Triggers>
                </ControlTemplate>
            </Setter.Value>
        </Setter>
    </Style>

