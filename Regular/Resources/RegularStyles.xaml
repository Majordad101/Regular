    <Style x:Key="DuplicateRuleButton" TargetType="Button">
        <Setter Property="OverridesDefaultStyle" Value="True" />
        <Setter Property="Template">
            <Setter.Value>
                <ControlTemplate TargetType="{x:Type Button}">
                    <Canvas
                        Width="{TemplateBinding Width}"
                        Height="{TemplateBinding Height}"
                        HorizontalAlignment="Center"
                        VerticalAlignment="Center"
                        ClipToBounds="True">
                        <Rectangle
                            x:Name="BackgroundRectangle"
                            Width="{TemplateBinding Width}"
                            Height="{TemplateBinding Height}"
                            Margin="0"
                            HorizontalAlignment="Stretch"
                            VerticalAlignment="Stretch"
                            Fill="{StaticResource ButtonBackgroundGray}" />
                        <Polygon
                            x:Name="BackgroundSquare"
                            Fill="{StaticResource ButtonBackgroundGray}"
                            Points="8,10 20,10 20,21, 8,21"
                            Stroke="{StaticResource ButtonStrokeColorDefault}"
                            StrokeThickness="1.25" />
                        <Polygon
                            x:Name="ForegroundSquare"
                            Fill="{StaticResource ButtonBackgroundGray}"
                            Points="5,5 17,5 17,17, 5,17"
                            Stroke="{StaticResource ButtonStrokeColorDefault}"
                            StrokeThickness="1.25" />
                        <Line
                            x:Name="VerticalLine"
                            Stroke="{StaticResource ButtonStrokeColorDefault}"
                            StrokeThickness="1.25"
                            X1="11"
                            X2="11"
                            Y1="7.5"
                            Y2="14.5" />
                        <Line
                            x:Name="HorizontalLine"
                            Stroke="{StaticResource ButtonStrokeColorDefault}"
                            StrokeThickness="1.25"
                            X1="7.5"
                            X2="14.5"
                            Y1="11"
                            Y2="11" />
                    </Canvas>
                    <ControlTemplate.Triggers>
                        <Trigger Property="IsEnabled" Value="False">
                            <Setter TargetName="BackgroundSquare" Property="Line.Stroke" Value="{StaticResource ButtonStrokeColorDisabled}" />
                            <Setter TargetName="ForegroundSquare" Property="Line.Stroke" Value="{StaticResource ButtonStrokeColorDisabled}" />
                            <Setter TargetName="HorizontalLine" Property="Line.Stroke" Value="{StaticResource ButtonStrokeColorDisabled}" />
                            <Setter TargetName="VerticalLine" Property="Line.Stroke" Value="{StaticResource ButtonStrokeColorDisabled}" />
                        </Trigger>
                        <Trigger Property="IsMouseOver" Value="True">
                            <Setter TargetName="BackgroundRectangle" Property="Rectangle.Fill" Value="#999999" />
                            <Setter TargetName="BackgroundSquare" Property="Line.Stroke" Value="{StaticResource ButtonStrokeColorHighlight}" />
                            <Setter TargetName="ForegroundSquare" Property="Line.Stroke" Value="{StaticResource ButtonStrokeColorHighlight}" />
                            <Setter TargetName="HorizontalLine" Property="Line.Stroke" Value="{StaticResource ButtonStrokeColorHighlight}" />
                            <Setter TargetName="VerticalLine" Property="Line.Stroke" Value="{StaticResource ButtonStrokeColorHighlight}" />
                        </Trigger>
                    </ControlTemplate.Triggers>
                </ControlTemplate>
            </Setter.Value>
        </Setter>
    </Style>

    <Style x:Key="StopStartRuleButton" TargetType="Button">
        <Setter Property="OverridesDefaultStyle" Value="True" />
        <Setter Property="Template">
            <Setter.Value>
                <ControlTemplate TargetType="{x:Type Button}">
                    <Canvas
                        Width="{TemplateBinding Width}"
                        Height="{TemplateBinding Height}"
                        HorizontalAlignment="Center"
                        VerticalAlignment="Center"
                        ClipToBounds="True">
                        <Rectangle
                            x:Name="BackgroundRectangle"
                            Width="{TemplateBinding Width}"
                            Height="{TemplateBinding Height}"
                            Margin="0"
                            HorizontalAlignment="Stretch"
                            VerticalAlignment="Stretch"
                            Fill="{StaticResource ButtonBackgroundGray}" />
                        <Ellipse
                            x:Name="StopStartButtonEllipse"
                            Canvas.Left="5"
                            Canvas.Top="5"
                            Width="15"
                            Height="15"
                            Stroke="{StaticResource ButtonStrokeColorDefault}"
                            StrokeThickness="1" />
                        <Polygon
                            x:Name="Square"
                            Fill="{StaticResource ButtonStrokeColorDefault}"
                            Points="10,10 15,10 15,15 10,15" />
                        <Polygon
                            x:Name="Triangle"
                            Fill="{StaticResource ButtonStrokeColorDefault}"
                            Points="10,9 16,12.5 10,16" />
                    </Canvas>
                    <ControlTemplate.Triggers>
                        <Trigger Property="IsMouseOver" Value="True">
                            <Setter TargetName="BackgroundRectangle" Property="Rectangle.Fill" Value="#999999" />
                            <Setter TargetName="Square" Property="Line.Stroke" Value="{StaticResource ButtonStrokeColorHighlight}" />
                        </Trigger>
                        <!--<Trigger Property="{TemplateBinding IsEnabled}" Value="True">
                            <Setter TargetName="Square" Property="Visibility" Value="Hidden" />
                            <Setter TargetName="Triangle" Property="Visibility" Value="Visible" />
                        </Trigger>
                        <Trigger Property="{TemplateBinding IsEnabled}" Value="False">
                            <Setter TargetName="Square" Property="Visibility" Value="Visible" />
                            <Setter TargetName="Triangle" Property="Visibility" Value="Hidden" />
                        </Trigger>-->
                    </ControlTemplate.Triggers>
                </ControlTemplate>
            </Setter.Value>
        </Setter>
    </Style>

